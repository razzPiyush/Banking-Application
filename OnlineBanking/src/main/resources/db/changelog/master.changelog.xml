<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.5.xsd">

    <!-- Track database changes -->
    <property name="now" value="now()" dbms="postgresql"/>
    
    <!-- Schema Changes -->
    <include file="schema/001-create-tables.xml" relativeToChangelogFile="true"/>
    <include file="schema/002-alter-tables.xml" relativeToChangelogFile="true"/>
    
    <!-- Data Changes -->
    <include file="data/001-initial-data.xml" relativeToChangelogFile="true"/>
    <include file="data/002-test-data.xml" relativeToChangelogFile="true" context="development"/>
</databaseChangeLog>